{
  "name": "@hypervision/ar-chess-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "eslint src --max-warnings=0",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:unit": "vitest run --passWithNoTests",
    "test:e2e": "pnpm exec firebase --config ../../firebase.json emulators:exec --project hypervision-demo \"pnpm exec playwright test\""
  },
  "dependencies": {
    "@hypervision/ar-core": "workspace:*",
    "@hypervision/chess-domain": "workspace:*",
    "@hypervision/engine": "workspace:*",
    "@hypervision/modules": "workspace:*",
    "@hypervision/ui-kit": "workspace:*",
    "@react-three/drei": "9.88.17",
    "@react-three/fiber": "8.15.19",
    "chess.js": "^1.0.0",
    "firebase": "^10.13.1",
    "next": "^14.2.6",
    "react": "^18.3.1",
    "react-chessboard": "4.7.2",
    "react-dom": "^18.3.1",
    "stockfish": "^17.1.0",
    "three": "^0.160.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.46.0",
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.160.0",
    "jsdom": "^24.1.1",
    "local-ssl-proxy": "^2.0.5",
    "vitest": "^2.0.5"
  }
}
